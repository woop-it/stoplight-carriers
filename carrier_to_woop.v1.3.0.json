            "$ref": "#/components/parameters/apiVersion"
          },
          {
            "$ref": "#/components/parameters/pathDeliveryId"
          }
        ],
        "requestBody": {
          "required": true,
          "description": "Contient les informations du delta cost.",
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "amount",
                  "reason",
                  "currency",
                  "date"
                ],
                "properties": {
                  "amount": {
                    "type": "number",
                    "format": "float"
                  },
                  "reason": {
                    "$ref": "#/components/schemas/deltaCostReason"
